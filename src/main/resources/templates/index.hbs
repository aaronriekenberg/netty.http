<!DOCTYPE html>
<html>

<head>
  <title>{{mainPageInfo.title}}</title>
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link rel="stylesheet" type="text/css" href="/style.css">
</head>

<body>

  <h2>{{mainPageInfo.title}}</h2>

  {{#if commandInfo}}
    <h3>Commands:</h3>
    <ul>{{#each commandInfo}}
      <li><a href="/commands/{{id}}">{{description}}</a></li>{{/each}}
    </ul>
  {{/if}}

  {{#if staticFilesInMainPage}}
    <h3>Static Paths:</h3>
    <ul>{{#each staticFilesInMainPage}}
      <li><a href="{{url}}">{{filePath}}</a></li>{{/each}}
    </ul>
  {{/if}}

<!--
  {{#if proxies}}
    <h3>Proxies:</h3>
    <ul>{{#each proxies}}
      <li><a href="/proxies/{{id}}">{{description}}</a></li>{{/each}}
    </ul>
  {{/if}}
-->

  <h3>Debugging:</h3>
  <ul>
    <li><a href="/debug/config">configuration</a></li>
    <li><a href="/debug/environment">environment</a></li>
    <li><a href="/debug/gc">gc</a></li>
    <li><a href="/debug/memory">memory</a></li>
    <li><a href="/debug/thread">thread</a></li>
  </ul>

  <hr>

  <small>Page Generated: {{lastModified}}</small>
  <br>

<!--  <small>Git Hash: {{environment.git_hash}}</small> -->

</body>

</html>
